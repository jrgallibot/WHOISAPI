# Whois Lookup API Configuration
# Copy this file to .env and fill in your actual values

# Required: WhoisXML API key
# Get your free API key from: https://whoisxmlapi.com/
WHOIS_API_KEY=your_whoisxml_api_key_here

# Optional: Flask server configuration
PORT=5000

# Optional: MySQL logging configuration
# Leave these empty to disable database logging
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=your_mysql_password
MYSQL_DB=whois_logs

# Optional: Logging level
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO
